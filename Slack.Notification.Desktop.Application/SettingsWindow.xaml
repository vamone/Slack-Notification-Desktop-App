<Window x:Class="Slack.Desktop.Application.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SlackDesktopBubbleApplication"
        mc:Ignorable="d"
        Title="Settings" Height="300" Width="428.707" Background="#FF6EC9DC">
    <Grid>
        <TextBox x:Name="TextBoxToken" HorizontalAlignment="Left" Height="23" Margin="8,103,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="323"/>
        <Label x:Name="label" Content="Token" HorizontalAlignment="Left" Margin="8,77,0,0" VerticalAlignment="Top"/>
        <Button x:Name="ButtonSaveToken" Content="Save" HorizontalAlignment="Left" Margin="336,103,0,0" VerticalAlignment="Top" Width="75" Click="ButtonSaveToken_Click" Height="23"/>
        <Label x:Name="label1" Content="Created By Valentine Monych" HorizontalAlignment="Left" Margin="246,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="valentine@monych.se" HorizontalAlignment="Left" Margin="285,41,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
